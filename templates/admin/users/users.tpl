<!-- <%$smarty.template%> --><%include file="admin/admin_header.tpl" pageTitle="Red Rock West Saloon - NYC"%><div id="centercontent">	<div id="homeTitle" class="title"><h1>Welcome</h1></div>				<h1 class="top">User List</h1><p class="lr"><a href="admin.php?mode=email" class="white">Click here to send an email to the group</a>. <span><a href="admin.php?mode=addUser&admin=true" class="white">Click here to add a user to the group.</a></span></p>		<table class="form" id="users">	<tr>		<th>Added</th>		<th>Email adderss</th>		<th>Name</th>		<th>D.O.B</th>		<th>Gender</th>		<th>ID#</th>		<th>Action</th>	</tr><%section name=usersList loop=$usersLoop%><form action="admin.php" method="post"><input type="hidden" name="mode" value="users"><input type="hidden" name="id" value="<%$usersLoop[usersList].id%>">			<tr class="grey">		<td><%$usersLoop[usersList].reg_date|date_format%></td>		<td><a href="mailTo:<%$usersLoop[usersList].email%>"><%$usersLoop[usersList].email%></a></td>		<td><%$usersLoop[usersList].name%></td>		<td><%$usersLoop[usersList].dob|date_format%></td>		<td><%$usersLoop[usersList].gender%></td>		<td><%$usersLoop[usersList].id%></td>		<td><input type="submit" name="delete" value="DELETE" class="button" onClick="return confirm('Delete <%$usersLoop[usersList].email%>?')"></td>	</tr></form><%/section%>	<tr>		<th colspan="8">Search</th>	</tr>	<tr>	<form action="admin.php" method="post">	<input type="hidden" name="mode" value="users">	<input type="hidden" name="action" value="search">		<td><input type="button" name="search" value="Clear search" class="button"></td>		<td><input type="text" name="email" value="<%$email%>"></td>		<td><input type="text" name="name" value="<%$name%>"></td>				<td class="small ctr">Born between<br />		<select name="s_dob_month"><%include file="inc/monthSelect.tpl" value=$s_dob_month%></select>		<select name="s_dob_day"><%include file="inc/daySelect.tpl" value=$s_dob_day%></select>		<select name="s_dob_year"><%include file="inc/yearSelect.tpl" value=$s_dob_year%></select>		<br />and<br />		<select name="e_dob_month"><%include file="inc/monthSelect.tpl" value=$e_dob_month%></select>		<select name="e_dob_day"><%include file="inc/daySelect.tpl" value=$e_dob_day%></select>		<select name="e_dob_year"><%include file="inc/yearSelect.tpl" value=$e_dob_year%></select></td>				<td><select name="gender">		<option value=""></option>		<option value="m" <%if $gender eq "m"%>selected="selected"<%/if%>>M</option>		<option value="f" <%if $gender eq "f"%>selected="selected"<%/if%>>F</option>		</select>				</td>		<td class="small ctr"><input type="text" name="id" value="<%$id%>" class="num"></td>		<td colspan="2" class="ctr"><input type="submit" name="submit" value="Search" class="button"></td>	</form>	</tr></table><p class="ctr mt10"><%$fullCount%> people in database<%if $search %> match your search criteria<%/if%>.</p> <span id="pager"><%if $pages %>Page:  <a href="admin.php?mode=users&start=<%$prev%><%if $search %>&action=search<%/if%><%if $email %>&email=<%$email%><%/if%><%if $username %>&username=<%$username%><%/if%><%if $status %>&status=<%$status%><%/if%><%if $emailList %>&emailList=<%$emailList%><%/if%>">&#8249;&#8249;&#8249;</a><%/if%> <%section name=pageLoop loop=$pages%><%if $pages[pageLoop].start eq "$curr"%><span class="currPage"><%/if%><a href="admin.php?mode=users&start=<%$pages[pageLoop].start%>"><%$pages[pageLoop].pagenumber%></a><%if $curr eq $pages[pageLoop].start%></span><%/if%><%/section%> <%if $pages %><a href="admin.php?mode=users&start=<%$next%><%if $search %>&action=search<%/if%><%if $email %>&email=<%$email%><%/if%><%if $username %>&username=<%$username%><%/if%><%if $status %>&status=<%$status%><%/if%><%if $emailList %>&emailList=<%$emailList%><%/if%>">&#8250;&#8250;&#8250;</a><%/if%></span><p class="ctr"><%$emailCount%> people are on the email list.</p></div>			</div><%include file="admin/admin_footer.tpl" currPage=$smarty.template%>